{
  "name": "typescript-jspm-umd-seed",
  "version": "0.0.1",
  "description": "Your golden ticket to the future, today.",
  "main": "dist/typescript-jspm-umd-seed.js",
  "typings": "dist/typescript-jspm-umd-seed.d.ts",
  "jspmPackage": true,
  "author": "Joel Hernandez",
  "license": "MIT",
  "config": {
    "moduleName" : "typescript-jspm-umd-seed",
    "globalName" : "TypeScriptJspmUmdSeed",
    "storageDir" : "storage/",
    "distDir" : "dist/",
    "sourceDir" : "src/",
    "sourceFiles" : "**/*.ts",
    "sourceFilesDtsExclude" : "**/*Spec.ts",
    "bundleFileName" : "bundle.js",
    "karmaConfigFile" : "karma.config.js"
  },
  "scripts": {
    "jspm": "jspm",
    "karma": "karma",
    "typings": "typings",
    "browser-sync": "browser-sync",
    "dts-generator": "dts-generator",
    "tsconfig": "tsconfig",
    "onchange": "onchange",
    "tslint": "tslint",
    "postinstall": "jspm install",
    "lint": "npm run tslint -- -c tslint.json $npm_package_config_sourceDir$npm_package_config_sourceFiles",
    "lint:watch": "onchange $npm_package_config_sourceDir$npm_package_config_sourceFiles -- npm run lint",
    "dts:fix:path": "replace $npm_package_config_sourceDir '' $npm_package_config_distDir$npm_package_config_moduleName.d.ts",
    "dts:fix:entry": "replace '/main' '' $npm_package_config_distDir$npm_package_config_moduleName.d.ts",
    "dts:fix": "npm-run-all dts:fix:path dts:fix:entry",
    "dts:build": "dts-generator --name $npm_package_config_moduleName --project . --exclude 'typings/**/*.d.ts' --exclude $npm_package_config_sourceDir$npm_package_config_sourceFilesDtsExclude --exclude 'node_modules/**/*.d.ts' --exclude 'jspm_packages/**/*.d.ts' --out $npm_package_config_distDir$npm_package_config_moduleName.d.ts",
    "build:dts": "npm-run-all dts:build dts:fix",
    "build:ts": "jspm build $npm_package_config_moduleName $npm_package_config_distDir$npm_package_config_moduleName.js --format umd --global-name $npm_package_config_globalName",
    "build:tsconfig": "tsconfig .",
    "build": "npm-run-all build:tsconfig lint build:ts build:dts",
    "bundle:dist": "jspm bundle $npm_package_config_moduleName $npm_package_config_storageDir$npm_package_config_bundleFileName",
    "bundle:dev": "jspm bundle $npm_package_config_moduleName $npm_package_config_storageDir$npm_package_config_bundleFileName -wid",
    "browser-sync:dist": "browser-sync start --server",
    "browser-sync:dev": "browser-sync start --server --files \"index.html|$npm_package_config_storageDir$npm_package_config_bundleFileName\"",
    "serve:dist": "npm-run-all bundle:dist browser-sync:dist",
    "serve:dev": "npm-run-all --parallel bundle:dev browser-sync:dev",
    "pretest": "npm run bundle:dist",
    "test": "karma start $npm_package_config_karmaConfigFile --single-run",
    "test:watch": "npm-run-all --parallel \"bundle:dev\" \"karma start $npm_package_config_karmaConfigFile\""
  },
  "devDependencies": {
    "browser-sync": "^2.11.2",
    "dts-generator": "^1.7.0",
    "jasmine-core": "^2.4.1",
    "jspm": "^0.17.0-beta.12",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-jspm": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "npm-run-all": "^1.7.0",
    "onchange": "^2.4.0",
    "phantomjs-prebuilt": "^2.1.7",
    "replace": "^0.3.0",
    "tsconfig-glob": "^0.4.3",
    "tslint": "^3.8.1",
    "tslint-eslint-rules": "^1.2.0",
    "typescript": "^1.8.10"
  },
  "jspm": {
    "name": "typescript-jspm-umd-seed",
    "dependencies": {},
    "devDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^4.0.5",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "readline": "github:jspm/nodelibs-readline@^0.2.0-alpha"
    },
    "peerDependencies": {},
    "overrides": {
      "npm:typescript@1.8.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "dependencies": {}
}
